# change window focus within space
alt - k: yabai -m window --focus south
alt - i : yabai -m window --focus north
alt - j : yabai -m window --focus west
alt - l : yabai -m window --focus east
alt - b : open "/Applications/Safari.app"
alt - 1 : yabai -m space --focus 1
alt - 2 : yabai -m space --focus 2
alt - 3 : yabai -m space --focus 3
alt - 4 : yabai -m space --focus 4
alt - 5 : yabai -m space --focus 5
alt - e : open "/Applications/DeepSeek.app/"
alt - f : open "/System/Library/CoreServices/Finder.app"
alt - u : open "/Applications/Balatro.app"
alt - c : open "/Applications/Visual Studio Code.app/"
alt - s : open "/Applications/Spotify.app"
shift + alt - 1 : yabai -m window --space 1
shift + alt - 2 : yabai -m window --space 2
shift + alt - 3 : yabai -m window --space 3
shift + alt - 4 : yabai -m window --space 4
shift + alt - 5 : yabai -m window --space 5
alt - o : open "/Applications/kitty.app"
shift + alt - s : open "/Applications/Stremio.app"
alt - p : open "/Applications/kitty.app"
ctrl + alt - q : yabai --stop-service
ctrl + alt - s : yabai --start-service
ctrl + alt - r : yabai --restart-service
